{
  "expo": {
    "name": "Tikket",
    "slug": "Tikket",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-tikket.jpg",
    "scheme": "testapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.julianojosoa13.Tikket"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-tikket.jpg",
        "backgroundColor": "#70D099"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.julianojosoa13.Tikket"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo-tikket.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo-tikket.jpg",
          "imageWidth": 400,
          "resizeMode": "contain",
          "backgroundColor": "gray"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a099f918-af52-4144-ade7-05514ac3ad38"
      }
    },
    "owner": "julianojosoa13"
  }
}
